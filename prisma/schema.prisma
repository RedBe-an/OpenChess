generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("OPENCHESS_DATABASE_URL")
  directUrl = env("OPENCHESS_DIRECT_URL")
}

model opening {
  eco     String
  name    String
  urlName String
  pgn     String  @unique
  mdx     String?
  fen     String

  @@map("opening")
}

{
  "name": "openchess",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "bun run build:parallel",
    "build:parallel": "bun run db:generate && bun run build:next",
    "build:next": "next build",
    "build:prod": "bun run db:setup && bun run build:next",
    "build:fast": "bun run db:generate && bun run build:next",
    "start": "next start",
    "lint": "prettier . --write",
    "lint:check": "prettier . --check",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:migrate": "bunx prisma migrate dev",
    "db:sync": "bun run bin/sync-openings.ts",
    "db:make": "bun run bin/namu-it.ts",
    "db:setup": "bun run db:generate && bun run db:migrate && bun run db:sync",
    "clean": "rimraf .next out dist",
    "prebuild": "bun run clean",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
  "bin": {
    "sync-openings": "./bin/sync-openings.ts",
    "namu-it": "./bin/namu-it.ts"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.1.4",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/client": "^6.12.0",
    "@prisma/extension-accelerate": "^2.0.1",
    "@prisma/extension-optimize": "^1.1.4",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.3",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.4",
    "@supabase/supabase-js": "^2.47.15",
    "@types/mdx": "^2.0.13",
    "@vercel/speed-insights": "^1.2.0",
    "chess.js": "^1.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.471.1",
    "next": "15.1.4",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.4",
    "node-fetch": "^3.3.2",
    "react": "^19.0.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "remark-toc": "^9.0.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@next/bundle-analyzer": "^15.1.4",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.7",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-config-next": "15.1.4",
    "postcss": "^8.5.1",
    "prettier": "3.4.2",
    "prisma": "^6.12.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.0",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vercel": "^39.3.0"
  }
}
